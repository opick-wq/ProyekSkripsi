{
    "version": 2,
    "builds": [
        {
            "src": "proyek_lokator/wsgi.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "15mb", 
                "runtime": "python3.9",
                "includeFiles": "db.sqlite3" 
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "proyek_lokator/wsgi.py"
        }
    ]
}
